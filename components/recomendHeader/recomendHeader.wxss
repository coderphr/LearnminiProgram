/* components/recomendHeader/recomendHeader.wxss */

.header {
  display: flex;
  flex-direction: column;
}
.title {
  margin: 20rpx 0 0 20rpx;
  font-size: 35rpx;
  color: #89818b;
}
.header .view {
  margin-left: 20rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
}

.header .more {
  padding: 10rpx;
  border: 1px solid #000000;
  border-radius: 20rpx;
  font-size: 22rpx;
}